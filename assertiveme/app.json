{
  "expo": {
    "name": "AssertiveMe",
    "slug": "assertive-me",
    "version": "1.0.0",
    "description": "Emotional awareness & assertiveness training app",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "assertiveme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "primaryColor": "#009ECF",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.assertiveme.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#009ECF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.assertiveme.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#009ECF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
